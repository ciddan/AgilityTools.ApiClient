	<SearchRequest base="/Structures/Classification/JULA Produkter">
		<SearchControls excludeBin="true" excludeDocument="true" />
		<Filter>
			<FilterString><![CDATA[FIND BELOW #11 WHERE("Attributgrupp" = "160701")]]></FilterString>
		</Filter>
	</SearchRequest>
</BatchRequest>

